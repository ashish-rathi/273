<!doctype html>
<html>
<head>
<link href="/styles/customstyle.css" rel="stylesheet" type="text/css" >
<meta charset="utf-8">
<title>Sign Up</title>
</head>
<script src="/jquery-ui-1.11.1/external/jquery/jquery.js"></script>
<script src="/jquery-ui-1.11.1/jquery-ui.js"></script>
<script src="/validator/jquery.validate.js"></script>
<script>
$(document).ready(function(){
       var x = $("#message").html();
       if(x!='')
       {
               $('#notification').slideToggle(500).delay(2000).slideToggle(500);
       }
  $("#form1").validate()
})
</script>
<style>
.error{color:red}
</style>
<body>
<div class="formfont">
<table width="100%" border="0">
  <tbody>
    <tr>
      <td width="62%">
      	<div class="formfont" style="font-size: 30px;">
      		<img src="images/mylogo.png" width="217" height="88"/>
      	Get started with eWay</div>
        <br>
      <div class="txtboxes" style="font-weight: bold; margin-left:2em">
      Create your personal account!
      </div>
      </td>
      <td width="45%">
      <div class="formfont">
      <br><br>Already have an account?</div>
      <br>
      <button style="margin-left:1em" class="ButtonBuss" name="signin" type="submit" id="signin">Sign In</button>
      </td>
    </tr>
    <tr>
      <td rowspan="4">
      
        <form id="form1" name="form1" method="post" >
        <table><tr><td>
        <br>First Name<br>
        <input type="text" name="txtfname" id="txtfname" class="txtboxes required"><br>
        
        <br>SSN<br>
        <input type="ssn" name="txtssn" id="txtssn" class="txtboxes required"><br>
        
        
        <br>Email<br>
        <input type="email" name="txtemail" id="txtemail" class="required txtboxes"><br>
        <br>Create your password<br>
        <input type="password" name="txtpwd" id="txtpwd" class="txtboxes required"><br>
        <br>Confirm password<br>
        <input type="password" name="txtpwd2" id="txtpwd2" class="txtboxes required"><br>
        
        
        
        <br>Zipcode<br>
        <input type="number" name="txtzip" id="txtzip" class="txtboxes required"><br>
      
      </td>
     </tr>
 	 <tr> 
      <td>
      Last Name<br>
        <input type="text" name="txtlname" id="txtlname" class="txtboxes required"><br><br>Upload your photo<br>
        <input type="file" name="img" id="img" class="txtboxes required"><br>
        <br>Street Address<br>
        <input type="text" name="txtmaddress" id="txtmaddress" class="txtboxes required"><br>
        <br>City<br>
        <input type="text" name="txtcity" id="txtcity" class="txtboxes required"><br>
        <br>State<br>
        <input type="text" name="txtstate" id="txtstate" class="txtboxes required"><br>
       <br><br>
        <button class="ButtonBuss" name="signup" type="submit" id="signup" >Submit</button>
       
      </td>
    </tr>
    </table>
     </form>
     </td>
     </tr>
  </tbody>
</table>
  </div>
      </body>
</html>